{
  "name": "reinaldorti/codigoaberto-novo",
  "description": "Uma simples peça de login para seu quebra-cabeças, Código Aberto :)",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Reinaldo Dorti",
      "email": "reinaldorti@gmail.com",
      "role": "Developer",
      "homepage": "https://www.dortistudio.com.br"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
