{
  "name": "reinaldorti/codigoaberto",
  "description": "Uma simples peça de login para seu quebra-cabeças, Código Aberto :)",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Reinaldo Dorti",
      "email": "reinaldorti@gmail.com",
      "role": "Developer",
      "homepage": "https://www.dortistudio.com.br"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "Source\\": "source/"
    },
    "files": [
      "source/Config/Config.php",
      "source/Config/Helpers.php",
      "source/Minify/Web.php",
      "source/Minify/Admin.php"
    ]
  },
  "require": {
    "ext-json": "*",
    "php": "^7.2.5",
    "ext-pdo": "^7.2.5",
    "phpmailer/phpmailer": "~6.0",
    "league/plates": "v4.0.0-alpha",
    "league/oauth2-facebook": "2.0.1",
    "league/oauth2-google": "3.0.2",
    "matthiasmullie/minify": "^1.3",
    "coffeecode/router": "1.0.7",
    "coffeecode/datalayer": "1.1.4",
    "coffeecode/paginator": "^1.0",
    "coffeecode/optimizer": "2.0.0",
    "coffeecode/uploader": "1.0.*",
    "coffeecode/cropper": "1.2.*",
    "symfony/var-dumper": "^5.1",
    "google/recaptcha": "^1.2"
  }
}
